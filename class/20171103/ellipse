void setup() {
  size(600, 600);
  background(0);
}

void draw() {
 ellipse(); //ellipse() loading
}

void ellipse(){ //make ellipse()
    for (int i = 0; i<600; i+=25){
  int c = int(map(i,0,600,0,255));
//  float c =map(i,0,600,0,255);
  fill(c,0,0);
  ellipse(width/2,height/2,width-i,height-i);
}
}
