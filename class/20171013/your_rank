float grade = int(random(0, 101));
print("당신의 점수는 " + grade + "점입니다. ");

if (grade >=90){
  println("당신의 등급은 A입니다.");
}else if (grade>=80 ){
  println("당신의 등급은 B입니다.");
  }else if (grade>=80 ){
  println("당신의 등급은 C입니다.");
  }else if (grade>=70 ){
  println("당신의 등급은 D입니다.");
  }else if (grade>=60 ){
  println("당신의 등급은 E입니다.");
  }else {
  println("당신의 등급은 F입니다.");
}
